{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_",
		"include": ["./js/antController.js", "./js/globals.js"]
    },
	"sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
		"default": {
            "includeDate": false
        }
	},
	"plugins": [
        "plugins/markdown",
		"plugins/underscore"
    ],
	"opts": {
        "template": "templates/antSim",
        //"template": "../ink-docstrap/template",
        "encoding": "utf8",
		"destination": "./user-code_doc/",	
	    "recurse": true,
        "tutorials": "tutorials" // same as -u path/to/tutorials
    }
}

